<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="black_user.manager.user">%black_user.user.manager%</parameter>
    </parameters>

    <services>
        <service id="black_user.entity.manager" alias="doctrine.orm.entity_manager" />

        <service id="black_user.manager.user" class="%black_user.manager.user%">
            <argument type="service" id="black_user.entity.manager" />
            <argument>%black_user.model.user.class%</argument>
        </service>
    </services>
</container>
