<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="black_user.register.unlock_form.type">Black\Bundle\UserBundle\Form\Type\UnlockAccountType</parameter>
    </parameters>

    <services>
        <service id="black_user.register.unlock_form.type" class="%black_user.register.unlock_form.type%">
            <argument>%black_user.model.user.class%</argument>
            <tag name="form.type" alias="black_user_unlock" />
        </service>
    </services>
</container>
